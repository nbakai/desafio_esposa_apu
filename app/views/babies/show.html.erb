<p id="notice"><%= notice %></p>

<h2>
  <strong>Nombre:</strong>
  <%= @baby.name %>
</h2>
<table>
  <thead>
    <tr>
      <th>Tiempo</th>
      <th>Cantidad</th>
  
      <th>Fecha</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
   <% @baby.breastfeedings.each do |breastfeeding| %>
      <tr>
        <td><%= breastfeeding.tiempo %> minutos</td>
        <td><%= breastfeeding.cantidad %> cc.</td>
        <td><%= breastfeeding.created_at%></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Añadir más', breastfeedings_path%> 

<%= link_to 'Regresar a babies', breastfeedings_path %>
